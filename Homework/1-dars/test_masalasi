function chegirma(summa) {
    if (summa > 100000) {
        console.log("10% chegirma: " + (summa - (summa * 0.10)) + " so'm");
    } else if (summa >= 50000 && summa <= 100000) {
        console.log("5% chegirma: " + (summa - (summa * 0.05)) + " so'm");
    } else {
        console.log("Chegirma yo'q: " + summa + " so'm");
    }
}

function yosh(tugilganYil) {
    let hozirgiYil = new Date().getFullYear();
    let yosh = hozirgiYil - tugilganYil;

    if (yosh < 18) {
        console.log("Yosh: " + yosh + " â€” Voyaga yetmagan");
    } else {
        console.log("Yosh: " + yosh);
    }
}
function login(login, parol) {
    if (login === "admin" && parol === "1234") {
        console.log("Hush kelibsiz!");
    } else {
        console.log("Login yoki parol noto'g'ri");
    }
}

function harorat(harorat) {
    if (harorat < 0) {
        console.log("sovuq");
    } else if (harorat > 0 && harorat <= 30) {
        console.log("Yoqimli harorat");
    } else {
        console.log("Juda issiq");
    }
}

function vaqt(soat) {
    if (soat >= 6 && soat < 12) {
        console.log("Ertalab");
    } else if (soat >= 12 && soat < 18) {
        console.log("Kunduzi");
    } else if ((soat >= 18 && soat <= 23) || (soat >= 0 && soat < 6)) {
        console.log("Kechasi");
    } else {
        console.log("Noto'g'ri soat kiritildi");
    }
}


function oy(oy) {
    if (oy === 12 || oy === 1 || oy === 2) {
        console.log("Qish");
    } else if (oy === 3 || oy === 4 || oy === 5) {
        console.log("Bahor");
    } else if (oy === 6 || oy === 7 || oy === 8) {
        console.log("Yoz");
    } else if (oy === 9 || oy === 10 || oy === 11) {
        console.log("Kuz");
    } else {
        console.log("Noto'g'ri oy raqami");
    }
}

chegirma(120000);  
yosh(2010);          
login("admin", "1234"); 
harorat(25);           
vaqt(21);
oy(4);                